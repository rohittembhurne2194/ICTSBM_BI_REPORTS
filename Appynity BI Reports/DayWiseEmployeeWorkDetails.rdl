          <DataField>Date</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="userid">
          <DataField>userid</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="Name">
          <DataField>Name</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="Total_Collection">
          <DataField>Total_Collection</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
    </DataSet>

  <DataSets>
    <DataSet Name="Collection_Details">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@from">
            <Value>=Parameters!from.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@to">
            <Value>=Parameters!to.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>--DECLARE @from DATETIME, @to DATETIME;
--SET @from = '2024-09-04';
--SET @to = '2024-09-06';

SELECT
    Date,
    userId,
    Name,
    SUM(Total_Collection) AS Total_Collection
FROM (
    SELECT 
        CAST(gc.gcDate AS DATE) AS Date,
        gc.userId,
        um.username AS Name,
        COUNT(gc.gcId) AS Total_Collection
    FROM GarbageCollectionDetails gc
    JOIN usermaster um ON um.userId = gc.userId
    WHERE CAST(gc.gcDate AS DATE) BETWEEN @from AND @to
      AND um.EmployeeType IS NULL
      AND gc.gcType = 1
    GROUP BY CAST(gc.gcDate AS DATE), gc.userId, um.username

    UNION ALL

    SELECT 
        CAST(nsc.gcDate AS DATE) AS Date,
        nsc.userId,
        um.username AS Name,
        COUNT(nsc.gcNotScanId) AS Total_Collection
    FROM GarbageCollection_NotScan nsc
    JOIN usermaster um ON um.userId = nsc.userId
    WHERE CAST(nsc.gcDate AS DATE) BETWEEN @from AND @to
      AND um.EmployeeType IS NULL
      AND nsc.isNotScan = 1
    GROUP BY CAST(nsc.gcDate AS DATE), nsc.userId, um.username
) AS CombinedData
GROUP BY Date, userId, Name
ORDER BY Date, userId;

</CommandText>
      </Query>
      <Fields>

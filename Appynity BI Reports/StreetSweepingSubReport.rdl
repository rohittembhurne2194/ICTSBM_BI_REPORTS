    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource2</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@userid">
            <Value>=Parameters!userid.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@date">
            <Value>=Parameters!date.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>--declare @date datetime
--declare @userid int
--set @date ='2022-05-25' 
--set @userid=17


select GC.gcDate,(select UM.userName From UserMaster UM where UM.userId=GC.userId) As EmpName , 
(Select SSD.ReferanceId From StreetSweepingDetails SSD Where SSD.SSId=GC.SSId) As ReferanceId ,
(Select SSD.SSName From StreetSweepingDetails SSD Where SSD.SSId=GC.SSId) As SSName,
(Select SSD.SSAddress From StreetSweepingDetails SSD Where SSD.SSId=GC.SSId) As SSAddress ,
GC.vehicleNumber , GC.note
from GarbageCollectionDetails GC where GC.EmployeeType = 'S' And Gc.userId=@userid and cast(GC.gcDate as date) = @date

OPTION(RECOMPILE)</CommandText>
